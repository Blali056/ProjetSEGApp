<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DeleteService">

    <android.widget.RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".addSevice"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="98dp">

        <TextView
            android:id="@+id/connexion2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/logoImage3"
            android:layout_alignParentTop="true"
            android:layout_marginStart="6dp"
            android:layout_marginTop="167dp"
            android:text="Entrez le nom du service:"
            android:textAppearance="@style/TextAppearance.AppCompat.Title.Inverse"
            android:textColor="@color/blackGrey" />

        <ImageView
            android:id="@+id/backgroundColor9"
            android:layout_width="match_parent"
            android:layout_height="137dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="0dp"
            android:layout_marginLeft="0dp"
            android:layout_marginBottom="0dp"
            app:srcCompat="@color/blackGrey" />

        <ImageView
            android:id="@+id/backgroundColor11"
            android:layout_width="match_parent"
            android:layout_height="99dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="0dp"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="0dp"
            app:srcCompat="@color/blackGrey" />

        <ImageView
            android:id="@+id/backgroundColor12"
            android:layout_width="55dp"
            android:layout_height="279dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="0dp"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="96dp"
            app:srcCompat="@color/blackGrey" />

        <ImageView
            android:id="@+id/backgroundColor14"
            android:layout_width="55dp"
            android:layout_height="279dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="329dp"
            android:layout_marginLeft="329dp"
            android:layout_marginTop="100dp"
            app:srcCompat="@color/blackGrey" />

        <ImageView
            android:id="@+id/backgroundColor13"
            android:layout_width="55dp"
            android:layout_height="279dp"
            android:layout_above="@+id/backgroundColor1"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginEnd="0dp"
            android:layout_marginRight="0dp"
            app:srcCompat="@color/blackGrey" />

        <ImageView
            android:id="@+id/logoImage3"
            android:layout_width="249dp"
            android:layout_height="51dp"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="22dp"
            app:srcCompat="@drawable/logo"
            tools:ignore="VectorDrawableCompat" />

        <EditText
            android:id="@+id/servicedeleteET"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/logoImage3"
            android:layout_alignParentTop="true"
            android:layout_marginStart="7dp"
            android:layout_marginTop="250dp"
            android:ems="10"
            android:inputType="textPersonName" />

        <Button
            android:id="@+id/deletebtn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="133dp"
            android:layout_marginLeft="133dp"
            android:layout_marginBottom="60dp"
            android:text="Supprimer" />

    </android.widget.RelativeLayout>
</android.support.constraint.ConstraintLayout>